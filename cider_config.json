{
    "title": "CIder 0.1 Example Docker Config",
    "backend": "bash",
    "pipelines": ["Test_Program"],
    "Test_Program": {
        "actions": ["Run_Tests", "Consolidate_Reports"],
        "Run_Tests": { 
